<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Version>2.0.2</Version>
    <Authors>Adam O'Neil</Authors>
    <Copyright>Copyright (c) 2020 Adam O'Neil</Copyright>
    <RepositoryUrl>https://github.com/adamosoftware/ModelSync</RepositoryUrl>
    <PackageProjectUrl>https://github.com/adamosoftware/ModelSync</PackageProjectUrl>
    <Description>A database schema diff library for .NET assemblies using AO.DbSchema.Attributes and SQL Server</Description>
    <PackageId>AO.ModelSync.Library</PackageId>
    <PackageLicenseFile>LICENSE</PackageLicenseFile>
    <PackageReleaseNotes>Refactored abstract class ConnectionModelBuilder into interface IConnectionModelBuilder</PackageReleaseNotes>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="AO.DbSchema.Attributes" Version="1.0.7" />
    <PackageReference Include="Dapper" Version="2.0.30" />
    <PackageReference Include="Microsoft.Data.SqlClient" Version="1.1.1" />
    <PackageReference Include="System.ComponentModel.Annotations" Version="4.7.0" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\LICENSE">
      <Pack>True</Pack>
      <PackagePath></PackagePath>
    </None>
  </ItemGroup>

</Project>
